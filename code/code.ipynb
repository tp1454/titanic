    "## End-to-end sklearn Pipeline: transform test like train and create submission\n",
    "from sklearn.base import BaseEstimator, TransformerMixin\n",
    "from sklearn.preprocessing import OneHotEncoder, StandardScaler\n",
    "from sklearn.compose import ColumnTransformer\n",
    "from sklearn.pipeline import Pipeline\n",
    "from sklearn.ensemble import RandomForestClassifier\n",
    "import numpy as np\n",
    "import pandas as pd\n",
    "import os\n",
    "\n",
    "class TitanicFeatureEngineer(BaseEstimator, TransformerMixin):\n",
    "    def __init__(self):\n",
    "        self.embarked_mode_ = None\n",
    "        self.age_median_by_group_ = None\n",
    "        self.fare_median_by_pclass_ = None\n",
    "        self.fare_quantile_bins_ = None  # edges for 4-quantile FareBand\n",
    "\n",
    "    @staticmethod\n",
    "    def _extract_title(name):\n",
    "        if pd.isna(name):\n",
    "            return 'Unknown'\n",
    "        try:\n",
    "            title = name.split(',')[1].split('.')[0].strip()\n",
    "        except Exception:\n",
    "            return 'Unknown'\n",
    "        if title in ['Mr','Mrs','Miss','Master']:\n",
    "            return title\n",
    "        return 'Rare'\n",
    "\n",
    "    @staticmethod\n",
    "    def _ticket_prefix(t):\n",
    "        t = str(t)\n",
    "        parts = [p for p in t.replace('.', '').replace('/', '').split() if not p.isdigit()]\n",
    "        if len(parts) == 0:\n",
    "            return 'NONE'\n",
    "        return parts[0]\n",
    "\n",
    "    def fit(self, X, y=None):\n",
    "        df = X.copy()\n",
    "        # Embarked mode\n",
    "        self.embarked_mode_ = df['Embarked'].mode().iloc[0] if df['Embarked'].notna().any() else 'S'\n",
    "\n",
    "        # Title for grouping\n",
    "        title = df['Name'].apply(self._extract_title)\n",
    "\n",
    "        # Age median by (Pclass, Title)\n",
    "        age = df['Age'].copy()\n",
    "        grp = pd.DataFrame({'Pclass': df['Pclass'], 'Title': title, 'Age': age})\n",
    "        self.age_median_by_group_ = grp.groupby(['Pclass','Title'])['Age'].median()\n",
    "\n",
    "        # Fare median by Pclass (for imputation)\n",
    "        self.fare_median_by_pclass_ = df.groupby('Pclass')['Fare'].median()\n",
    "\n",
    "        # FareBand bins (quartiles) from train\n",
    "        fare = df['Fare'].fillna(df['Fare'].median())\n",
    "        # compute quantile edges (0, .25, .5, .75, 1)\n",
    "        qs = np.quantile(fare, [0, 0.25, 0.5, 0.75, 1.0])\n",
    "        # ensure strictly increasing (handle duplicates by jitter)\n",
    "        eps = 1e-6\n",
    "        for i in range(1, len(qs)):\n",
    "            if qs[i] <= qs[i-1]:\n",
    "                qs[i] = qs[i-1] + eps\n",
    "        self.fare_quantile_bins_ = qs\n",
    "        return self\n",
    "\n",
    "    def transform(self, X):\n",
    "        df = X.copy()\n",
    "        # Embarked fill\n",
    "        df['Embarked'] = df['Embarked'].fillna(self.embarked_mode_)\n",
    "\n",
    "        # Title\n",
    "        df['Title'] = df['Name'].apply(self._extract_title)\n",
    "\n",
    "        # Family features\n",
    "        df['FamilySize'] = df['SibSp'] + df['Parch'] + 1\n",
    "        df['IsAlone'] = (df['FamilySize'] == 1).astype(int)\n",
    "\n",
    "        # Age imputation via (Pclass, Title) median with fallback to global median\n",
    "        def _fill_age_row(row):\n",
    "            if pd.isna(row['Age']):\n",
    "                med = self.age_median_by_group_.get((row['Pclass'], row['Title']), np.nan)\n",
    "                if pd.isna(med):\n",
    "                    return float(self.age_median_by_group_.median())\n",
    "                return float(med)\n",
    "            return float(row['Age'])\n",
    "        df['Age'] = df.apply(_fill_age_row, axis=1)\n",
    "\n",
    "        # Fare imputation by Pclass median\n",
    "        def _fill_fare_row(row):\n",
    "            if pd.isna(row['Fare']):\n",
    "                med = self.fare_median_by_pclass_.get(row['Pclass'], np.nan)\n",
    "                if pd.isna(med):\n",
    "                    return float(df['Fare'].median())\n",
    "                return float(med)\n",
    "            return float(row['Fare'])\n",
    "        df['Fare'] = df.apply(_fill_fare_row, axis=1)\n",
    "\n",
    "        # AgeBin fixed bins\n",
    "        df['AgeBin'] = pd.cut(df['Age'], bins=[0,12,20,40,60,120], labels=['Child','Teen','Adult','MidAge','Senior'])\n",
    "\n",
    "        # FareBand using train quantile edges\n",
    "        df['FareBand'] = pd.cut(df['Fare'], bins=self.fare_quantile_bins_, labels=False, include_lowest=True)\n",
    "\n",
    "        # TicketPrefix\n",
    "        df['TicketPrefix'] = df['Ticket'].apply(self._ticket_prefix)\n",
    "\n",
    "        # Sex binary\n",
    "        df['Sex'] = df['Sex'].map({'male':1,'female':0}).astype(int)\n",
    "\n",
    "        # Keep only columns needed downstream\n",
    "        keep = ['PassengerId','Pclass','Sex','Age','Fare','Embarked','Title','FamilySize','IsAlone','AgeBin','FareBand','TicketPrefix']\n",
    "        return df[keep]\n",
    "\n",
    "# Build preprocessing + model pipeline\n",
    "cat_cols = ['Embarked','Title','TicketPrefix','AgeBin']\n",
    "num_cols_scale = ['Age','Fare','FamilySize']\n",
    "passthrough_cols = ['Pclass','IsAlone','Sex','FareBand']  # numeric, leave as-is\n",
    "\n",
    "preprocess = ColumnTransformer(\n",
    "    transformers=[\n",
    "        ('onehot', OneHotEncoder(drop='first', handle_unknown='ignore', sparse_output=False), cat_cols),\n",
    "        ('scale', StandardScaler(), num_cols_scale),\n",
    "        ('pass', 'passthrough', passthrough_cols),\n",
    "    ],\n",
    "    remainder='drop',\n",
    "    verbose_feature_names_out=False\n",
    ")\n",
    "\n",
    "rf_model = RandomForestClassifier(\n",
    "    n_estimators=850,\n",
    "    max_depth=12,\n",
    "    min_samples_split=2,\n",
    "    min_samples_leaf=2,\n",
    "    max_features='sqrt',\n",
    "    class_weight='balanced',\n",
    "    n_jobs=-1,\n",
    "    random_state=SEED,\n",
    ")\n",
    "\n",
    "pipe = Pipeline([\n",
    "    ('fe', TitanicFeatureEngineer()),\n",
    "    ('prep', preprocess),\n",
    "    ('clf', rf_model),\n",
