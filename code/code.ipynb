    "# Hyperparameter tuning with cross-validation\n",
    "from sklearn.model_selection import RandomizedSearchCV\n",
    "from scipy.stats import randint, uniform\n",
    "\n",
    "param_dist = {\n",
    "    'n_estimators': randint(300, 900),\n",
    "    'max_depth': [None] + list(range(4, 13)),\n",
    "    'min_samples_split': randint(2, 11),\n",
    "    'min_samples_leaf': randint(1, 5),\n",
    "    'max_features': ['sqrt','log2', 0.5, 0.7],\n",
    "    'bootstrap': [True],\n",
    "    'class_weight': ['balanced', None],\n",
    "}\n",
    "\n",
    "rf_tune = RandomForestClassifier(n_jobs=-1, random_state=SEED)\n",
    "rs = RandomizedSearchCV(\n",
    "    rf_tune, param_distributions=param_dist, n_iter=30,\n",
    "    scoring='roc_auc', cv=5, random_state=SEED, n_jobs=-1, verbose=1, refit=True\n",
    ")\n",
    "rs.fit(X_train, y_train)\n",
    "\n",
    "best_rf = rs.best_estimator_\n",
    "print('Best params:', rs.best_params_)\n",
    "print('Best CV ROC AUC:', round(rs.best_score_,4))\n",
    "\n",
    "# Evaluate tuned model\n",
    "pred2 = best_rf.predict(X_valid)\n",
    "proba2 = best_rf.predict_proba(X_valid)[:,1]\n",
    "acc2 = accuracy_score(y_valid, pred2)\n",
    "auc2 = roc_auc_score(y_valid, proba2)\n",
    "print({'tuned_accuracy': round(acc2,4), 'tuned_roc_auc': round(auc2,4)})\n",
    "print('\\nClassification report (tuned):\\n', classification_report(y_valid, pred2, digits=3))\n",
    "print('\\nConfusion matrix (tuned):\\n', confusion_matrix(y_valid, pred2))\n",
    "\n",
    "joblib.dump({'model': best_rf, 'features': X.columns.tolist()}, 'processed/rf_best.pkl')\n",
    "print('Saved tuned model to processed/rf_best.pkl')"
