    "# 4. Select & encode features\n",
    "train_final = train.copy()\n",
    "\n",
    "# remove 'HasCabin','CabinLetter'\n",
    "keep_cols = [\n",
    "    'PassengerId','Survived','Pclass','Sex','Age','Fare',\n",
    "    'Embarked','Title','FamilySize','IsAlone',\n",
    "    'AgeBin','FareBand','TicketPrefix'\n",
    "]\n",
    "\n",
    "train_final = train_final[keep_cols].copy()\n",
    "\n",
    "# One-hot encode(remove 'CabinLetter')\n",
    "to_onehot = ['Embarked','Title','TicketPrefix','AgeBin']\n",
    "train_final = pd.get_dummies(train_final, columns=to_onehot, drop_first=True)\n",
    "\n",
    "# Sex to binary\n",
    "train_final['Sex'] = train_final['Sex'].map({'male':1,'female':0}).astype(int)\n",
    "\n",
    "# Check missing\n",
    "print('Any nulls left:', train_final.isna().sum().sum())\n",
    "\n",
    "# Save processed (unscaled)\n",
    "os.makedirs('processed', exist_ok=True)\n",
    "train_final.to_csv('processed/titanic_train_preprocessed.csv', index=False)\n",
    "print('Saved processed/titanic_train_preprocessed.csv')"
